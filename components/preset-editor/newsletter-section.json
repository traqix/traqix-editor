            "props": {
              "className": "text-4xl font-bold mb-4"
            },
            "allowsChildren": false,
            "children": [
              {
                "id": "text-newsletter-title",
                "name": "Text Newsletter Title",
                "type": "text",
                "props": {
                  "content": "Subscribe to Our Newsletter"
                }
              }
            ]
          },
          {
            "id": "newsletter-description",
            "name": "Newsletter Description",
