                  "onclick": "var city = JSON.parse(window.localStorage.getItem('traqix_mem_city')).value ?? ''; if (city !== '') { fetch(`https://api.openweathermap.org/data/2.5/weather?q=${city}&appid=0d019a678e59d7111413243a36716e41&units=metric`).then(response => response.json()).then(data => { window.localStorage.setItem('traqix_mem_weather', JSON.stringify({type: 'object', value: data})); window.localStorage.setItem('traqix_mem_city_name', JSON.stringify({type: 'string', value: data.name})); window.localStorage.setItem('traqix_mem_temperature', JSON.stringify({type: 'number', value: data.main.temp})); window.localStorage.setItem('traqix_mem_min_temp', JSON.stringify({type: 'number', value: data.main.temp_min})); window.localStorage.setItem('traqix_mem_max_temp', JSON.stringify({type: 'number', value: data.main.temp_max})); window.localStorage.setItem('traqix_mem_humidity', JSON.stringify({type: 'number', value: data.main.humidity})); window.localStorage.setItem('traqix_mem_wind_speed', JSON.stringify({type: 'number', value: data.wind.speed})); window.localStorage.setItem('traqix_mem_conditions', JSON.stringify({type: 'string', value: data.weather[0].description})); var result = `City: ${data.name}\\nTemperature: ${data.main.temp} °C\\nMin Temp: ${data.main.temp_min} °C\\nMax Temp: ${data.main.temp_max} °C\\nHumidity: ${data.main.humidity}%\\nWind Speed: ${data.wind.speed} m/s\\nConditions: ${data.weather[0].description}`; window.localStorage.setItem('traqix_mem_display', JSON.stringify({type: 'string', value: result})); }).catch(error => { window.localStorage.setItem('traqix_mem_display', JSON.stringify({type: 'string', value: 'Error fetching weather data'})); }); }"
